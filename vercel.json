{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "routes": [
    {
      "src": "/assets/(.*)",
      "dest": "/assets/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "env": {
    "VITE_API_URL": "https://81.200.155.237:8000",
    "VITE_TELEGRAM_BOT_NAME": "antidps_bot",
    "VITE_APP_URL": "https://antidps-web.vercel.app"
  }
}
